{
  "extends": "airbnb",
  "rules": {
      "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
      "no-param-reassign": 0,
      "no-plusplus": 0,
      "consistent-return": 0,
      "no-console": 0,
      "jsx-a11y/media-has-caption": 0,
    },
  "globals": {
    "window": true
  }
}
